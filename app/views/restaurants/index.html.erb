<h1>Make a Reservation</h1>
<div id="wrap">
	<div id="columns" class="columns_2">
<% @all.each do |x| %>
		<figure>
		<figcaption>
		<%= link_to x.name, restaurant_reservations_path(x) %>
		</figcaption>
			<%= link_to image_tag(x.image), restaurant_reservations_path(x) %><br>	
		</figure>
		<% end %> 
	</div>
	</div>

		


